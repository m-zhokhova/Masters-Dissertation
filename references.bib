@incollection{otalAnalysisGeneRegulatory2025,
  title = {Analysis of {{Gene Regulatory Networks}} from {{Gene Expression Using Graph Neural Networks}}},
  author = {Otal, Hakan T. and Subasi, Abdulhamit and Kurt, Furkan and Canbaz, M. Abdullah and Uzun, Yasin},
